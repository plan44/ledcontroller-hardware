
(rules PCB ledcontroller
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 248)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.6)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
    (clear 150.2 (type PowerSmall_PowerSmall))
  )
  (padstack "Via[0-1]_600:400_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_1000:600_um"
    (shape
      (circle F.Cu 1000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_600:400_um" "Via[0-1]_600:400_um" default
  )
  (via 
    "Via[0-1]_1000:600_um" "Via[0-1]_1000:600_um" default
  )
  (via 
    "Via[0-1]_600:400_um-kicad_default" "Via[0-1]_600:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_1000:600_um-kicad_default" "Via[0-1]_1000:600_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:400_um-Power" "Via[0-1]_600:400_um" Power
  )
  (via 
    "Via[0-1]_1000:600_um-Power" "Via[0-1]_1000:600_um" Power
  )
  (via 
    "Via[0-1]_600:400_um-PowerFET" "Via[0-1]_600:400_um" PowerFET
  )
  (via 
    "Via[0-1]_1000:600_um-PowerFET" "Via[0-1]_1000:600_um" PowerFET
  )
  (via 
    "Via[0-1]_600:400_um-PowerSmall" "Via[0-1]_600:400_um" PowerSmall
  )
  (via 
    "Via[0-1]_1000:600_um-PowerSmall" "Via[0-1]_1000:600_um" PowerSmall
  )
  (via_rule
    default "Via[0-1]_600:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:400_um-kicad_default"
  )
  (via_rule
    Power "Via[0-1]_1000:600_um-Power"
  )
  (via_rule
    PowerFET "Via[0-1]_1000:600_um-PowerFET"
  )
  (via_rule
    PowerSmall "Via[0-1]_600:400_um-PowerSmall"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(C1-Pad1)" "Net-(C2-Pad1)" "Net-(D1-Pad1)" "Net-(D1-Pad3)" "Net-(D2-Pad1)" "Net-(D2-Pad2)" "Net-(J2-Pad9)" "/ETH0_TX+"
    "/ETH0_TX-" "/ETH0_RX+" "/ETH0_RX-" "/ETH0_LED" "/WLAN_LED" "/Omega_RX" "/Omega_TX" "Net-(R1-Pad1)"
    "Net-(R2-Pad1)" /USB+ "/USB-" "Net-(R9-Pad2)" "Net-(C8-Pad1)" "Net-(J2-Pad12)" "/LED_DATA1" "/LED_FET_5V"
    "/LED_FET_3V" "/LED_DATA1_3V" "/LED_DATA2_3V" "/LED_DATA3_3V" "/LED_DATA2" "Net-(J6-Pad2)" "Net-(J6-Pad3)" "/SPI_MOSI"
    "/SPI_MISO" "/SPI_CLK" "/SPI_CS1" "/LED_DATA3"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Power
    GND +3V3 +24V +5V "/CHS_GND"
    (clearance_class Power)
    (via_rule Power)
    (rule
      (width 1000.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class PowerFET
    "/FET_PWR"
    (clearance_class PowerFET)
    (via_rule PowerFET)
    (rule
      (width 5000.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class PowerSmall
    "/SENS_PWR"
    (clearance_class PowerSmall)
    (via_rule PowerSmall)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)